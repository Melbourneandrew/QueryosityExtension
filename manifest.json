{
  "manifest_version": 2,
  "name": "Queryosity",
  "version": "1.0.0",
  "icons": {"255": "icon.png"},
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "dashboard.html"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "tabs"
  ]
}
