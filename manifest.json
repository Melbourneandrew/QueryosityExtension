{
  "manifest_version": 2,
  "name": "Queryosity",
  "version": "2.0.0",
  "description": "Track and analyze your search habits! Nothing pertaining to the content of your search is stored, only the date and time is logged!",
  "icons": {"255": "qicon.png"},
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action":{
    "default_icon": "qicon.png",
    "default_popup": "dashboard.html"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "tabs"
  ]
}
